{
  "name": "sanity-nextjs-vercel-example",
  "version": "0.0.0",
  "private": true,
  "description": "A bare bones example of a Vercel-deployable project with a Next.js frontend and a Sanity Studio on /studio",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "lerna": "^3.22.1"
  },
  "scripts": {
    "dev": "concurrently \"next --port 9000\" \"cd studio && sanity start\"",
    "build:next": "next dev -p 9000",
    "build": "echo 'Building Sanity to public/admin' && cd studio && sanity build ../public/admin -y && cd .. && next build && next export -o build",
    "start": "vercel dev"
  },
  "keywords": [],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/base": "^2.5.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/cli": "^2.0.5",
    "@sanity/client": "^2.2.6",
    "@sanity/components": "^2.2.6",
    "@sanity/core": "^2.5.0",
    "@sanity/default-layout": "^2.5.0",
    "@sanity/default-login": "^2.2.6",
    "@sanity/desk-tool": "^2.5.0",
    "@sanity/image-url": "^0.140.22",
    "@sanity/vision": "^2.2.6",
    "groq": "^2.2.6",
    "next": "^9.5.5",
    "prop-types": "^15.7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-responsive-carousel": "^3.2.13",
    "vercel": "^21.3.2"
  }
}
